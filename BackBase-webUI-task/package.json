{
  "name": "backbase-web-task",
  "version": "1.0.0",
  "description": "Web UI testing framework for the purpose of testing some functionalities of  BBlog",
  "main": "index.js",
  "scripts": {
    "test": "testcafe chrome tests/*",
    "test-safari": "testcafe safari tests/*",
    "test-chrome-headless": "testcafe chrome:headless tests/*",
    "test-safari-headless": "testcafe safari:headless tests/*",
    "test-with-report": "testcafe chrome tests/* --reporter html:reports/report.html",
    "test-chrome-headless-with-report": "testcafe chrome:headless tests/* --reporter html:reports/report.html"
  },
  "author": "Suhila Ahmed",
  "license": "ISC",
  "devDependencies": {
    "testcafe": "^1.20.0"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "randomstring": "^1.2.2",
    "testcafe-reporter-html": "^1.4.6"
  }
}
